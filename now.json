{
  "alias": "repo.modularfilemaker.org",
  "scale": {
    "sfo1": {
      "min": 1,
      "max": 2
    }
  },
  "env": {
    "PORT": 3050,
    "FILEMAKER_APPLICATION": "@filemaker-application",
    "FILEMAKER_SERVER": "@filemaker-server",
    "FILEMAKER_USERNAME": "@filemaker-username",
    "FILEMAKER_PASSWORD": "@filemaker-password",
    "REDIS_HOST": "@redis-host",
    "REDIS_PORT": "@redis-port",
    "REDIS_AUTH_PASS": "@redis-auth-password"
  }
}
